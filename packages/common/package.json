{
  "name": "@cenk1cenk2/oclif-common",
  "version": "1.0.0",
  "description": "Oclif common package for oclif2 projects.",
  "repository": "https://gitlab.kilic.dev/libraries/oclif-tools",
  "type": "module",
  "author": {
    "name": "Cenk Kilic",
    "email": "cenk@kilic.dev",
    "url": "https://srcs.kilic.dev"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "build": "tsdown",
    "dev:start": "tsdown --watch",
    "format": "prettier --write src/ --log-level warn && eslint --fix src/",
    "lint": "eslint src/",
    "docs:api": "typedoc --options .typedoc.json --hideInPageTOC --hideBreadcrumbs"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --log-level warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --log-level warn --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@oxc-project/runtime": "^0.82.2",
    "deepmerge": "^4.3.1",
    "object-path-immutable": "^4.1.2",
    "reflect-metadata": "^0.2.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@listr2/manager": "^3.0.1",
    "@oclif/core": "^4.5.2",
    "@types/fs-extra": "^11.0.4",
    "@types/through": "^0.0.33",
    "@types/update-notifier": "^6.0.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "fs-extra": "^11.3.1",
    "listr2": "^9.0.1",
    "source-map-support": "^0.5.21",
    "yaml": "^2.8.1"
  },
  "peerDependencies": {
    "@listr2/manager": ">= 1",
    "@oclif/core": ">= 1",
    "class-transformer": ">= 0.5.0",
    "class-validator": ">= 0.14.0",
    "enquirer": ">= 2",
    "fs-extra": ">= 10",
    "listr2": ">= 7",
    "update-notifier": ">= 5",
    "yaml": ">= 2",
    "source-map-support": ">= 0.5.0"
  },
  "peerDependenciesMeta": {
    "update-notifier": {
      "optional": true
    },
    "yaml": {
      "optional": true
    },
    "source-map-support": {
      "optional": true
    }
  }
}
