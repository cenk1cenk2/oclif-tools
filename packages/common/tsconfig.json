{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@commands": ["commands"],
      "@constants": ["constants"],
      "@interfaces": ["interfaces"],
      "@lib": ["lib"],
      "@lib/*": ["lib/*"],
      "@utils": ["utils"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
