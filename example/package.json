{
  "name": "oclif2",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "description": "oclif example Hello World CLI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "homepage": "https://github.com/cenk1cenk2/hello-world",
  "repository": "cenk1cenk2/hello-world",
  "author": "Cenk Kılıç @cenk1cenk2",
  "keywords": [
    "oclif"
  ],
  "bin": {
    "oclif2": "./bin/run"
  },
  "files": [
    "/config",
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "oclif2",
    "dirname": "oclif2",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "hooks": {
      "init": [
        "./dist/hooks/update-notifier.hook"
      ],
      "command_not_found": "./dist/hooks/not-found.hook"
    },
    "topicSeparator": " "
  },
  "scripts": {
    "dev": "./bin/dev.js",
    "start": "./bin/run.js",
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "clean": "rimraf oclif.manifset.json",
    "format": "prettier --write src/ --log-level warn && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "manifest": "oclif manifest"
  },
  "simple-git-hooks": {
    "pre-commit": "lint-staged",
    "prepare-commit-msg": "[ -t 1 ] && exec < /dev/tty && git cz --hook || true"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --log-level warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --log-level warn --write"
    ]
  },
  "dependencies": {
    "@cenk1cenk2/oclif-common": "workspace:*",
    "@oclif/core": "^3.10.6",
    "@oclif/plugin-help": "^6.0.5",
    "@oclif/plugin-plugins": "^4.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "enquirer": "^2.4.1",
    "listr2": "^7.0.2"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^1.7.2",
    "@cenk1cenk2/eslint-config": "^2.7.26",
    "@tsconfig/node16": "^16.1.1",
    "@types/fs-extra": "^11.0.3",
    "@types/node": "^20.8.10",
    "commitizen": "^4.3.0",
    "eslint": "^8.53.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "globby": "^13.2.2",
    "lint-staged": "^15.0.2",
    "oclif": "^4.0.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
