{
  "name": "oclif2",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "description": "oclif example Hello World CLI",
  "type": "module",
  "homepage": "https://github.com/cenk1cenk2/hello-world",
  "repository": "cenk1cenk2/hello-world",
  "author": "Cenk Kılıç @cenk1cenk2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "oclif"
  ],
  "bin": {
    "oclif2": "./bin/run"
  },
  "files": [
    "/config",
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "oclif2",
    "dirname": "oclif2",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "hooks": {
      "init": [
        "./dist/hooks/update-notifier.hook"
      ],
      "command_not_found": "./dist/hooks/not-found.hook"
    },
    "topicSeparator": " "
  },
  "scripts": {
    "dev": "./bin/dev.js",
    "start": "./bin/run.js",
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "clean": "rimraf oclif.manifset.json",
    "format": "prettier --write src/ --log-level warn && eslint --fix src/",
    "lint": "eslint src/",
    "manifest": "oclif manifest"
  },
  "simple-git-hooks": {
    "pre-commit": "lint-staged",
    "prepare-commit-msg": "[ -t 1 ] && exec < /dev/tty && git cz --hook || true"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --log-level warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --log-level warn --write"
    ]
  },
  "dependencies": {
    "@cenk1cenk2/oclif-common": "workspace:*",
    "@oclif/core": "^4.2.0",
    "@oclif/plugin-help": "^6.2.20",
    "@oclif/plugin-plugins": "^5.4.23",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "enquirer": "^2.4.1",
    "listr2": "^8.2.5"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^1.8.0",
    "@cenk1cenk2/eslint-config": "^3.1.19",
    "@tsconfig/node16": "^16.1.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.10",
    "commitizen": "^4.3.1",
    "eslint": "^9.17.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "globby": "^14.0.2",
    "lint-staged": "^15.4.3",
    "oclif": "^4.17.18",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
